 type Category{
 	id: ID! @unique
 	name: String!
 	description: String!
 	icon: String!
 	slug: String!
 	products: [Product!]!
 }

 type Brand{
 	id: ID! @unique
 	name: String!
 	image_url: String!
 	slug: String!
 	products: [Product!]!
 }

 type Product{
 	id: ID! @unique
 	name: String!
 	description: String!
 	image_url: String!
 	price: Int!
 	stock: Int!
 	brand: Brand!
 	category: Category!
 	slug: String!
 	sale: Boolean!
 }

